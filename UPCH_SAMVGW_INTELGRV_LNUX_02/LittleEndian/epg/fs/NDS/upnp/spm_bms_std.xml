<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <actionList> 
    <action>
      <name>GetDeviceStatus</name>
      <argumentList>
        <argument>
          <name>DeviceStatus</name>
          <direction>out</direction>
          <relatedStateVariable>DeviceStatus</relatedStateVariable>
        </argument>
      </argumentList>
    </action> 
    <action>
      <name>X_NDS_DeviceKeepAliveRequest</name>
      <argumentList>
        <argument>
          <name>Request</name>
          <direction>in</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_DeviceKeepAliveRequest</relatedStateVariable>
        </argument>
        <argument>
          <name>DeviceId</name>
          <direction>in</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_DeviceId</relatedStateVariable>
        </argument>
        <argument>
          <name>DeviceType</name>
          <direction>in</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_DeviceType</relatedStateVariable>
        </argument>
        <argument>
          <name>DeviceUdn</name>
          <direction>in</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_DeviceUdn</relatedStateVariable>
        </argument>
        <argument>
          <name>MaxRunningDevicesOfType</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_DeviceMaxRun</relatedStateVariable>
        </argument>
        <argument>
          <name>Result</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_DeviceKeepAliveResult</relatedStateVariable>
        </argument>
      </argumentList>
    </action> 
    <action>
      <name>X_NDS_GetVgwId</name>
      <argumentList>
        <argument>
          <name>VgwId</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_StbId</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>X_NDS_GetSkipUnentitledChannels</name>
      <argumentList>
        <argument>
          <name>SkipUnentitledChannels</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_SkipUnentitledChannels</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>X_NDS_GetSiLanguage</name>
      <argumentList>
        <argument>
          <name>SiLang</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_SiLang</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>X_NDS_PairIpcRequest</name>
      <argumentList>
        <argument>
          <name>IpcId</name>
          <direction>in</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_StbId</relatedStateVariable>
        </argument>
        <argument>
          <name>PairStatus</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_PairStatus</relatedStateVariable>
        </argument>
        <argument>
          <name>VgwId</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_StbId</relatedStateVariable>
        </argument>
        <argument>
          <name>ClientLicenseLength</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_ClientLicenseLength</relatedStateVariable>
        </argument>
        <argument>
          <name>ClientLicenseData</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_ClientLicenseData</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>X_NDS_GetMocaConfiguration</name>
      <argumentList>
        <argument>
          <name>MocaPassword</name>
          <direction>out</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_MocaPassword</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>X_NDS_UpdateConnectionType</name>
      <argumentList>
        <argument>
          <name>ConnectionType</name>
          <direction>in</direction>
          <relatedStateVariable>X_NDS_A_ARG_TYPE_ConnectionType</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
  <serviceStateTable>
    <stateVariable sendEvents="yes">
      <name>DeviceStatus</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_RebootStatus</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>RebootNow</allowedValue>
        <allowedValue>RebootLater</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_ClientLicenseData</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_DeviceKeepAliveRequest</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>Connect</allowedValue>
        <allowedValue>Disconnect</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_DeviceKeepAliveResult</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>ConnectedOk</allowedValue>
        <allowedValue>ConnectedBlocked</allowedValue>
        <allowedValue>Disconnected</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_DeviceId</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_DeviceMaxRun</name>
      <dataType>ui4</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_DeviceType</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>IpZapper</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_DeviceUdn</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_StbId</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_SkipUnentitledChannels</name>
      <dataType>string</dataType>
    </stateVariable> 
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_SiLang</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_PairStatus</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>PAIRED</allowedValue>
        <allowedValue>FAILED_TO_PAIR</allowedValue>
        <allowedValue>FAILED_REACHED_MAX_PAIRED_ALLOWED</allowedValue>
        <allowedValue>INSTALL_MODE_REQUIRED</allowedValue>
        <allowedValue>CONNECTION_ERROR</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_ClientLicenseLength</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_MocaPassword</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>X_NDS_A_ARG_TYPE_ConnectionType</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>Ethernet</allowedValue>
        <allowedValue>MoCA</allowedValue>
        <allowedValue>WiFi</allowedValue>
      </allowedValueList>
    </stateVariable>  
  </serviceStateTable>
</scpd>