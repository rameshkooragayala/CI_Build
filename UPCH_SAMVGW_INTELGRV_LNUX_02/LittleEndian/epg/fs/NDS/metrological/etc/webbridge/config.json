{
 "port":80,
 "binding":"0.0.0.0",
 "idletime":0,
 "persistentpath":"/NDS/metrological/root",
 "datapath":"/NDS/metrological/usr/share/webbridge",
 "systempath":"/NDS/metrological/usr/lib/webbridge",
 "proxystubpath":"/NDS/metrological/usr/lib/webbridge/proxystubs",
 "redirect":"/Service/Controller/UI",
 "oomadjust":15,
 "plugins":[
  {
   "callsign":"Tracing",
   "locator":"libtracecontrol.so",
   "classname":"TraceControl",
   "configuration":{
   }
  },
  {
   "callsign":"Monitor",
   "locator":"libmonitor.so",
   "classname":"Monitor",
   "autostart":true,
   "configuration":{
    "observables":[
     {
      "callsign":"WebKitBrowser",
      "interval":5000,
      "memory":307200
     },
     {
      "callsign":"YouTube",
      "interval":5000,
      "memory":307200
     },
     {
      "callsign":"Netflix",
      "interval":5000,
      "memory":307200
     }
    ]
   }
  },
  {
   "callsign":"QueueCommunicator",
   "locator":"libqueuecommunicator.so",
   "classname":"QueueCommunicator",
   "configuration":{
    "browser":"WebKitBrowser",
    "queue":"/webkitprocess",
    "monitor":"/webkitmonitor",
    "proxy":"/webkitplayer",
    "mapfile":"keymap.json"
   }
  },
  {
   "callsign":"DeviceInfo",
   "locator":"libdeviceinfo.so",
   "classname":"DeviceInfo"
  },
  {
   "callsign":"Provisioning",
   "locator":"libprovisioning.so",
   "classname":"Provisioning",
   "configuration":{
    "operator":"liberty",
    "provisionpath":"/tmp/provision",
    "server":"provisioning.metrological.com:80",
    "autoprovision":false
   }
  },
  {
   "callsign":"WebProxy",
   "locator":"libwebproxy.so",
   "classname":"WebProxy",
   "configuration":{
    "connections":10,
    "links":[
     {
      "name":"upnp",
      "type":"udp",
      "host":"239.255.255.250:1900"
     },
     {
      "name":"serial",
      "type":"serial",
      "device":"/dev/ttyACM0",
      "configuration":{
       "baudrate":115200,
       "parity":"none",
       "data":8,
       "stop":1
      }
     }
    ]
   }
  },
  {
   "callsign":"Netflix",
   "locator":"libnrd.so",
   "classname":"Netflix",
   "autostart":false,
   "configuration":{
    "provisioning":"Provisioning",
    "startsuspended":false,
    "version":"1.8.6-dev",
    "model":"Liberty Global_Horizon",
    "keyrepeatdelay":1200,
    "keyrepeatinterval":200
   }
  },
  {
   "callsign":"WebServer",
   "locator":"libwebserver.so",
   "classname":"WebServer",
   "configuration":{
    "port":8080,
    "binding":"0.0.0.0",
    "path":"/boot/www",
    "proxies":[
     {
      "path":"/Service/DeviceInfo",
      "subst":"/Service/DeviceInfo",
      "address":"127.0.0.1:80"
     },
     {
      "path":"/Service/DIALServer",
      "subst":"/Service/DIALServer",
      "address":"127.0.0.1:80"
     }
    ]
   }
  },
  {
   "callsign":"WebKitBrowser",
   "locator":"libwebkitbrowser.so",
   "classname":"WebKitBrowser",
   "autostart":true,
   "configuration":{
    "startsuspended":false,
    "url":"about:blank",
    "useragent":"Mozilla/5.0 (Macintosh, Intel Mac OS X 10_11_4) AppleWebKit/602.1.28+ (KHTML, like Gecko) Version/9.1 Safari/601.5.17 WPE-Horizon-Unified",
    "injectedbundle":"libWPEInjectedBundle.so",
    "compositor":"noaa",
    "inspector":"0.0.0.0:9998",
    "fps":true,
    "cursor":false,
    "touch":false,
    "msebuffers":"audio:2m,video:15m,text:1m",
    "memoryprofile":"128m",
    "memorypressure":"databaseprocess:30m,networkprocess:50m,webprocess:170m,rpcprocess:50m",
    "mediadiskcache":false,
    "diskcache":"90m",
    "xhrcache":true
   }
  },
  {
   "callsign":"YouTube",
   "locator":"libwebkitbrowser.so",
   "classname":"WebKitBrowser",
   "autostart":false,
   "configuration":{
    "startsuspended":false,
    "url":"about:blank",
    "useragent":"Mozilla/5.0 (Macintosh, Intel Mac OS X 10_11_4) AppleWebKit/602.1.28+ (KHTML, like Gecko) Version/9.1 Safari/601.5.17 WPE-Horizon-Unified",
    "injectedbundle":"libWPEInjectedBundle.so",
    "compositor":"noaa",
    "inspector":"0.0.0.0:9999",
    "fps":true,
    "cursor":false,
    "touch":false,
    "msebuffers":"audio:2m,video:15m,text:1m",
    "memoryprofile":"128m",
    "memorypressure":"databaseprocess:30m,networkprocess:50m,webprocess:170m,rpcprocess:50m",
    "mediadiskcache":false,
    "diskcache":"90m",
    "xhrcache":true
   }
  },
  {
   "callsign":"DIALServer",
   "locator":"libdialserver.so",
   "classname":"DIALServer",
   "autostart":false,
   "configuration":{
    "webserver":"WebServer",
    "name":"Horizon",
    "model":"Generic Platform",
    "manufacturer":"Metrological",
    "description":"WebBridge DIAL reference server.",
    "apps":[
     {
      "name":"YouTube",
      "allowstop":true
     },
     {
      "name":"Netflix",
      "allowstop":true
     }
    ]
   }
  }
 ]
}