# Supported MIME types
@image
image/jpeg
image/png

@video
video/x-ms-asf
video/vnd.dlna.mpeg-tts
video/x-ms-wmv
video/x-ms-wma
video/avi
video/msvideo
video/x-msvideo
video/x-flv
video/flv
video/x-matroska
video/mp4
video/mpeg
video/MP2T
video/quicktime
video/divx

@audio
audio/mp4
audio/mpeg
audio/aac
audio/aacp
audio/x-wav
audio/wav
audio/wave
audio/x-ms-wma
audio/L16
audio/vnd.dlna.adts
